		"flowbite-svelte-blocks": "^1.1.4",
		"flowbite-svelte-icons": "^2.2.0",
		"jscpd": "^4.0.5",
		"jsdom": "^26.1.0",
		"just-extend": "^6.2.0",
		"postcss": "^8.5.4",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"svelte": "^5.33.17",
		"svelte-check": "^4.2.1",
		"svelte-meta-tags": "^4.4.0",
		"tailwind-merge": "^3.3.0",
		"tailwindcss": "^4.1.8",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vitest": "^3.2.2"
